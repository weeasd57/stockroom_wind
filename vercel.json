{
  "version": 2,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/**/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_SKIP_STATIC_GENERATION": "true",
    "NEXT_FORCE_DYNAMIC": "true"
  }
}
